{
	// Route
	"route_Get": {
		"prefix": "get",
		"body": "get \"/${1:route}\", to: \"${2:controller}#${3:function}\"",
		"description": "rails route get",
		"scope": "meta.rails.routes"
  },
	"route_Post": {
		"prefix": "post",
		"body": "post \"/${1:route}\", to: \"${2:controller}#${3:function}\"",
